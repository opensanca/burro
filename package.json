  "bugs": {
    "url": "https://github.com/opensanca/burro/issues"
  },
  "homepage": "https://github.com/opensanca/burro#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "autoprefixer": "^7.2.5",
    "bluebird": "^3.5.1",
    "cdnjs-importer": "^2.0.0-beta",
    "cssnano": "^3.10.0",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "onchange": "^4.0.0",
    "postcss": "^6.0.16",
    "surge": "^0.19.0"
